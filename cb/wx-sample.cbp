			<Add directory="../src/external/glad" />
		</Compiler>
		<Linker>
			<Add option="`$(#WX_CONFIG) --libs std,gl`" />
			<Add option="-lopengl32" />
			<Add option="-lglu32" />
			<Add option="`pkg-config --libs glm`" />
			<Add option="-lthreepp" />
			<Add library="opengl32" />
			<Add library="freetype" />
			<Add directory="../build/bin" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions />
	</Project>
